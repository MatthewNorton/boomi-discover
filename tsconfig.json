{
    "compilerOptions": {
        "outDir": "./build/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "downlevelIteration": true,
        "target": "ESNext",
        "jsx": "react",
        "lib": ["es2015", "dom", "es2017"],
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    },
    "include": [
        "./src/**/*"
    ],
}